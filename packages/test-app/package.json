{
  "name": "test-app",
  "version": "0.3.1",
  "private": true,
  "description": "A test app to test prerendering.",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "start": "ember serve",
    "prerender": "ember prerender --url-fetcher=\"./lib/url-fetcher.js\"",
    "test:node": "node node-tests/index.js",
    "test:dev": "_TEST_SHOULD_PRERENDER=false ember test",
    "test:prerender": "_TEST_SHOULD_PRERENDER=true PRERENDER=true ember test",
    "test:ember": "yarn test:dev && yarn test:prerender",
    "generate-test-fixtures": "node ./lib/generate-test-fixtures.js"
  },
  "devDependencies": {
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.6.0",
    "@glimmer/component": "~1.0.4",
    "@glimmer/tracking": "~1.0.4",
    "broccoli-asset-rev": "~3.0.0",
    "chai": "~4.3.4",
    "ember-auto-import": "~2.4.0",
    "ember-build-prerender": "0.3.1",
    "ember-cli": "~4.1.0",
    "ember-cli-babel": "~7.26.11",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-htmlbars": "~6.0.1",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-terser": "~4.0.2",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-page-title": "~7.0.0",
    "ember-qunit": "~5.1.5",
    "ember-resolver": "~8.0.3",
    "ember-source": "~4.2.0",
    "fs-extra": "~10.0.0",
    "glob": "~7.2.0",
    "loader.js": "~4.7.0",
    "mocha": "~9.2.1",
    "qunit": "~2.18.0",
    "qunit-dom": "~2.0.0",
    "webpack": "^5.69.1"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
